{
    "name": "proxy-extend",
    "version": "1.2.0",
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/proxy-extend",
    "description": "Transparently extend a JS object with additional properties (using ES6 Proxy)",
    "keywords": [
        "es6",
        "proxy",
        "extend",
        "object",
        "wrapper",
        "metadata",
        "object",
        "extension",
        "annotate",
        "annotation",
        "typescript"
    ],
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/",
        "typings/index.d.ts"
    ],
    "types": "./typings/index.d.ts",
    "engines": {
        "node": ">= 10.13"
    },
    "main": "./lib-cjs/index.cjs",
    "module": "./lib-esm/index.mjs",
    "scripts": {
        "build:cjs": "rm -rf ./lib-cjs && NODE_ENV=production BABEL_ENV=cjs babel src --out-dir=lib-cjs --out-file-extension=.cjs",
        "build:esm": "rm -rf ./lib-esm && NODE_ENV=production BABEL_ENV=esm babel src --out-dir=lib-esm --out-file-extension=.mjs",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require @babel/register --recursive tests/unit && tsd && echo '[tsd] success'",
        "test:perf": "NODE_ENV=development node tests/perf",
        "prepublishOnly": "npm run build:cjs && npm run build:esm"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@babel/cli": "^7.8.4",
        "@babel/preset-env": "^7.9.5",
        "typescript": "^3.8.3",
        "tsd": "^0.11.0",
        "mocha": "^7.1.1",
        "chai": "^4.2.0",
        "benny": "^3.6.14"
    },
    "dependencies": {
        "ts-toolbelt": "^6.5.1",
        "message-tag": "^0.5.1"
    }
}
