{
    "name": "proxy-extend",
    "version": "0.7.0",
    "description": "Transparently extend a JS object with additional properties (using ES6 Proxy)",
    "keywords": [
        "es6",
        "proxy",
        "extend",
        "object",
        "wrapper",
        "metadata",
        "object",
        "extension",
        "annotate",
        "annotation",
        "typescript"
    ],
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/proxy-extend",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/",
        "typings/index.d.ts"
    ],
    "type": "module",
    "main": "./lib-cjs/index.cjs",
    "module": "./lib-esm/index.mjs",
    "exports": {
        ".": "./lib-esm/index.mjs"
    },
    "types": "./typings/index.d.ts",
    "scripts": {
        "rename:cjs": "find ./lib-cjs -name \"*.js\" -exec bash -c 'mv \"$1\" \"${1%.js}\".cjs' - '{}' \\;",
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs && npm run rename:cjs",
        "rename:esm": "find ./lib-esm -name \"*.js\" -exec bash -c 'mv \"$1\" \"${1%.js}\".mjs' - '{}' \\;",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm && npm run rename:esm",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require @babel/register --recursive tests/unit && tsd && echo '[tsd] success'",
        "test:perf": "NODE_ENV=development node tests/perf",
        "prepublishOnly": "npm run build:cjs && npm run build:esm"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/register": "^7.7.4",
        "@babel/cli": "^7.7.5",
        "@babel/preset-env": "^7.7.6",
        "typescript": "^3.7.3",
        "tsd": "^0.11.0",
        "mocha": "^6.2.2",
        "chai": "^4.2.0",
        "benny": "^3.6.14"
    },
    "dependencies": {
        "ts-toolbelt": "^4.12.13",
        "message-tag": "^0.4.2"
    }
}
